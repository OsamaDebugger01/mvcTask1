@{
}
<h2>Summary about Guests</h2>
@foreach(var item in Model){
    <h3>Guest Information</h3>
    <ul>
        <li>@item.name</li>
        <li>@item.email</li>
        <li>@item.phone</li>
        <li>@item.willAttend</li>
    </ul>
}